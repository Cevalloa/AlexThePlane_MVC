<script>
    $(".mapLink").hover( function (){
        $(this).css({ 'text-decoration': 'underline' });
        $(".videosLink").css({ 'text-decoration': 'none' });
  }, function () {
        var cssObj = {
            'text-decoration': 'none'
        }
        $(this).css(cssObj);
        $(".videosLink").css({ 'text-decoration': 'underline' });
         });

    $(".thumbsUpLink").hover(function () {
